@{
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
    Page.Title = "Probe - Templates Administration";

    string divhtml = "";

    var currTbl = Request.QueryString["tblName"];
    if(String.IsNullOrEmpty(currTbl)){
        currTbl = "NO TABLE DEFINED";
    }
}

<!DOCTYPE html>

<html>
<head>
    <title>Table Maintenance Admin</title>
    <link href="~/Content/themes/base/all.css?rfrsh=true" rel="stylesheet" type="text/css" />

    @*<link href="~/Content/themes/cupertino/jquery-ui-1.10.3.custom.css" rel="stylesheet" type="text/css">*@

    <link href="~/Content/Site.css?rfrsh=true" rel="stylesheet" type="text/css" />
    <link href="~/Content/TableMaint.css?rfrsh=true" rel="stylesheet" type="text/css" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <script src="~/Scripts/jquery-2.1.3.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.2.js"></script>
    <script src="~/Scripts/jquery.columnizer.js"></script>

    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/ProbeScripts/TableMaint.js?rfrsh=true"></script>

    <meta name="viewport" content="width=device-width" />

</head>
<body>
    @if (ViewBag.hasTableMaintEdit)
    {
        <div class="title-wrap">
            <h2>Tables Maintenance Administration</h2>
            <h3>Editing table: @currTbl</h3>

        </div>
        <div id="listDiv" class="listDiv">

        </div>
        <div id="dialog-modal">

        </div>
    }
    else
    {
        <div class="msgDiv">
            <p>
                You have not been given privilege to this option. <br /> If you feel you have received this message in error please contact the application administator.
            </p>
        </div>
    }

</body>
</html>